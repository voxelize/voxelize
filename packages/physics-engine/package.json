    "vitest": "^0.10.5"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@types/gl-vec3": "^1.1.4",
    "babel-loader": "^8.4.1",
    "file-loader": "^6.2.0",
