{
  "name": "@voxelize/core",
  "version": "0.1.223",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf ./dist",
    "compile": "vite build",
    "build": "yarn run clean && yarn run compile && yarn run types && yarn run copy",
    "types": "tsc --emitDeclarationOnly --outDir ./dist -p ./tsconfig.json --declaration --allowJs",
    "watch": "cross-env NODE_ENV=development npm-run-all --parallel watch:*",
    "watch:build": "pnpm types --watch --preserveWatchOutput",
    "watch:compile": "pnpm compile --watch",
    "copy": "cp ./src/styles.css ./dist",
    "test": "vitest --run",
    "prepublishOnly": "yarn run build",
    "preinstall": "npx only-allow pnpm"
  },
  "peerDependencies": {
    "postprocessing": "^6.35.0",
    "three": "^0.162.0"
  },
  "devDependencies": {
    "@types/gl-vec3": "^1.1.1",
    "@types/ndarray": "^1.0.11",
    "@types/omggif": "^1.0.3",
    "@types/three": "^0.165.0",
    "@types/uuid": "^8.3.4",
    "vite-plugin-string-replace": "^1.1.3"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^18.6.4",
    "@voxelize/aabb": "workspace:*",
    "@voxelize/physics-engine": "workspace:*",
    "@voxelize/protocol": "workspace:*",
    "@voxelize/raycast": "workspace:*",
    "deepmerge": "^4.2.2",
    "domurl": "^2.3.4",
    "events": "^3.3.0",
    "fflate": "^0.7.4",
    "gl-vec3": "^1.1.3",
    "jsonfn": "^0.31.0",
    "ndarray": "^1.0.19",
    "noisejs": "^2.1.0",
    "omggif": "^1.0.10",
    "regenerator-runtime": "^0.13.9",
    "socket.io-client": "^4.4.1",
    "uuid": "^8.3.2",
    "vite-plugin-copy": "^0.1.6"
  }
}
