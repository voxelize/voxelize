{
  "name": "docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "prepare:typedoc": "pnpm --filter @voxelize/protocol proto:generate && pnpm --filter @voxelize/aabb build && pnpm --filter @voxelize/raycast build && pnpm --filter @voxelize/physics-engine build && pnpm --filter @voxelize/ts-core build",
    "start": "pnpm run prepare:typedoc && cross-env NO_UPDATE_NOTIFIER=1 TYPEDOC_WATCH=true docusaurus start --port 3040",
    "build": "pnpm run prepare:typedoc && cross-env NO_UPDATE_NOTIFIER=1 docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "cross-env USE_SSH=true docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@docusaurus/core": "3.7.0",
    "@docusaurus/module-type-aliases": "3.7.0",
    "@docusaurus/plugin-content-docs": "3.7.0",
    "@docusaurus/preset-classic": "3.7.0",
    "@docusaurus/theme-mermaid": "3.7.0",
    "@docusaurus/tsconfig": "3.7.0",
    "@docusaurus/types": "3.7.0",
    "@mdx-js/react": "^3.1.0",
    "@voxelize/core": "workspace:*",
    "clsx": "^1.2.1",
    "docusaurus-plugin-typedoc": "^0.21.0",
    "postprocessing": "^6.37.1",
    "prism-react-renderer": "^2.4.1",
    "prism-themes": "^1.9.0",
    "protobufjs": "^7.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-type-animation": "^3.2.0",
    "styled-components": "^6.1.16",
    "three": "^0.165.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.7.0",
    "@docusaurus/tsconfig": "3.7.0",
    "@docusaurus/types": "3.7.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "typedoc": "0.25.13",
    "typedoc-plugin-markdown": "^3.17.1",
    "typedoc-plugin-no-inherit": "1.4.0",
    "typescript": "^5.4.5"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
